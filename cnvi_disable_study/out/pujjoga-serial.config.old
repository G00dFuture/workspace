CONFIG_CBFS_VERIFICATION=y
CONFIG_GBB_FLAG_DEV_SCREEN_SHORT_DELAY=y
CONFIG_GBB_FLAG_DISABLE_FW_ROLLBACK_CHECK=y
CONFIG_GBB_FLAG_FORCE_DEV_BOOT_USB=y
CONFIG_GBB_FLAG_FORCE_DEV_SWITCH_ON=y
CONFIG_VBOOT_FWID_VERSION=".16562.0.0"
CONFIG_RUN_FSP_GOP=y
CONFIG_PAYLOAD_NONE=y
CONFIG_USE_BLOBS=y

# Chrome OS
CONFIG_CHROMEOS=y

# FSP Blobs
CONFIG_ADD_FSP_BINARIES=y
CONFIG_FSP_M_FILE="3rdparty/blobs/intel/adln/fsp/fspm.bin"
CONFIG_FSP_S_FILE="3rdparty/blobs/intel/adln/fsp/fsps.bin"
CONFIG_RUN_FSP_GOP=y
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/intel/adln/fsp/FullHeader/Include/"

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=y

# Management Engine
CONFIG_HAVE_ME_BIN=y

# Management Engine FW update
CONFIG_SOC_INTEL_CSE_RW_UPDATE=y
CONFIG_SOC_INTEL_CSE_RW_VERSION="16.50.20.1649"

# Video Blob
CONFIG_INTEL_GMA_ADD_VBT=y

# Microcode
CONFIG_CPU_MICROCODE_CBFS_EXTERNAL_BINS=y

# Serial console disabled by default (do not remove)
# CONFIG_CONSOLE_SERIAL is not set

# Event Logging
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

CONFIG_SPI_FLASH_SMM=y

CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_PUJJOGA=y

# SPI Descriptor
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/google/nissa/descriptor-pujjoga.bin"

# Management Engine
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/google/nissa/csme-pujjoga.bin"

# Management Engine FW update
CONFIG_SOC_INTEL_CSE_RW_FILE="3rdparty/blobs/mainboard/google/nissa/me_rw-pujjoga.bin"

# Video Blob
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/google/nissa/vbt-pujjoga.bin"

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/google/nissa/ucode-pujjoga.bin"

CONFIG_CONSOLE_SERIAL=y
CONFIG_FATAL_ASSERTS=y

CONFIG_CONSOLE_SERIAL_115200=y
CONFIG_UART_FOR_CONSOLE=0

